DECL
  integer a, b, i;
  integer add1(integer a, c);
ENDDECL

integer add1(integer a, c){
  DECL
    integer b;
  ENDDECL
  BEGIN
    b = a + c;
    return b;
  END
}

integer main(){
  DECL
    integer c;
  ENDDECL
  BEGIN
    read(a);
    read(c);
    b = add1(a, c);
    write(b);
    return 0;
  END
}
