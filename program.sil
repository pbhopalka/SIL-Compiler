decl
  integer a[10], b, i, p;
  integer add1(integer &a, &c);
  integer support(integer &a, &b);
enddecl

integer support(integer &a, &b){
  begin
    return (a+b);
  end
}

integer add1(integer &a, &c){
  decl
    integer b;
  enddecl
  begin
    b = support(a, c);
    return b;
  end
}

integer main(){
  decl
    integer c;
    integer e;
  enddecl
  begin
    i = 0;
    read(c);
    read(e);
    b = add1(c, e);
    write(b);
    return 0;
  end
}
